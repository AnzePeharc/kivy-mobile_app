<HoldLayout>:
    canvas.before:
        Color:
            rgba: utils.get_color_from_hex('#d6d8db')
        Line:
            width: 1
            rectangle: self.x, self.y, self.width, self.height
    anchor_x: "center"
    id: hold_container

    MDIconButton:
        id: hold
        icon: "assets/Basic_ball.png"
        on_press: root.clicked()
        user_font_size: "30sp"

<ClimbingGrid>:
    canvas.before:
        Color:
            rgb: utils.get_color_from_hex('#373737')
        Rectangle:
            pos: self.pos
            size: self.size
    cols: 12
    padding: 20

    MDLabel:
        size_hint_x: None
        width: 30
        text:""
        font_style: "H6"
        font_name: "JetBrainsMono"
        theme_text_color: "Custom"
        text_color: utils.get_color_from_hex('#ffffff')
